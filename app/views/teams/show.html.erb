<section class="navTeamSec">
  <ul>
    <%= link_to "Выйти", destroy_user_session_path, method: :delete, class: 'whiteBorderBtn' %>
    <li>Лига: <span>Чемпионы</span></li>
    <li>Очки: <span>345 3535 54</span></li>
  </ul>
  <ul class="navigationWrap teamNav">
    <li><a href="">Календарь игр</a></li>
    <li><a href="">Лиги</a></li>
    <li><a href="">Сертификаты</a></li>
    <li><a href="">Рейтинг команд</a></li>
    <li><a href="">Франшиза и спонсорство</a></li>
  </ul>
</section>
<section class="topInfo team">
  <div class="contentWrap">
    <a href="#" class="start_edit">Редактировать</a>
    <h1 id="team_name_header" class="editable" data-name="team[name]" data-action="<%= team_path(@team) %>"><%= @team.name %></h1>
  </div>
  <ul class="contentWrap">
      <li><img src="<%= asset_path('team/achif.png') %>" /></li>
      <li><img src="<%= asset_path('team/achif.png') %>" /></li>
      <li><img src="<%= asset_path('team/achif.png') %>" /></li>
  </ul>
  <div class="contentWrap">
    <ul class="faceParth">
      <% @team.members.each do |member| %>
        <li>
          <div class="avatar"></div>
          <p class="<%= member.team_role %>"><%= member.user.name %></p>
        </li>
      <% end %>
      <% @team.invitations.each do |inv| %>
        <li class="not_member">
          <div class="avatar_wrapper"></div>
          <div class="avatar"></div>
          <p><%= inv.user.name %></p>
        </li>
      <% end %>
      <% unless @team.full? %>
        <li>
          <a href="#invite_modal" data-toggle="modal">
            <div class="avatar">
              <div class="cycle">
                <span></span>
                <span></span>
              </div>
              <sub>еще <%= "#{@team.places} #{Russian.p(@team.places, "место", "места", "мест")}" %></sub>
            </div>
          </a>
        </li>
    <% end %>      
    </ul>
  </div>
</section>
<section class="teamSec">
  <div class="contentWrap">
    <div class="left">
      <h4>Наши успехи</h4>
      <div class="teamSucsesTabs">
        <ul class="navTeamSucses" role="tablist">
          <li role="presentation" class="active">
            <a href="#reyting" aria-controls="reyting" role="tab" data-toggle="tab">Общий рейтинг</a>
          </li>
          <li role="presentation">
            <a href="#position" aria-controls="position" role="tab" data-toggle="tab">Положение в лиге</a>
